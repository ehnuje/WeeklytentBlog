<!DOCTYPE html>
<html>
<head>
  <title>Weeklytent</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <div class="container">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="nav-logo">
      <button type="button" class="navbar-toggle nav-toggle-zindex" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
      </button>
      <div class="text-center">
        <div id="nav-title">
          <%= image_tag "logo.png", :class => "img-responsive aligncenter img-rounded" %>
        </div>
      </div>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="nav-menu">
      <div class="text-center">
        <ul class="nav navbar-nav">
          <li>
            <%= link_to "Projects", folios_path %>
          </li>
          <li>

            <%= link_to "Members", members_path %>

          </li>
        </ul>
      </div>
    </div>
    <!-- /.navbar-collapse -->
  </div>
    <!-- /.container -->


  <div class="container">

    <%= yield %>

    <hr>
    <!-- Footer -->
    <footer>
      <div class="row">
          <div class="col-lg-12">
              <p>Copyright Â© Your Website 2014</p>
          </div>
      </div>
      <!-- /.row -->
    </footer>
  </div>




</body>
</html>
